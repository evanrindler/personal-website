---
layout: story
---

<div class="intro">
    <a href="http://apps.northbynorthwestern.com/commencement/2016/" target="_blank">
        <p>North by Northwestern</p>
        <span>Commencement 2016</span>
    </a>
</div>

<div class="container">
    <div class="entry">
        <div class="entry-details">
            <div class="entry-image-wrapper">
                <div class="entry-image">
                    <img class="circular" src="<%= entry_media %>" />
                </div>
            </div>
            <h2><%= entry_name %></h2>
            <h3>by <%= entry_author %></h3>
            <h4><a href="http://twitter.com/<%= entry_twitter %>" id="twitter-bio"><%= entry_twitter %></a></h4>
            <p class="bio"><%= entry_bio %></p>
            <div class="share-icons">
                <ul class="social">
                    <a href="http://twitter.com/home?status=North by Northwestern&#8217;s Class of 2016 http://apps.northbynorthwestern.com/commencement/2016/story/<%= entry_slug %> via @nbn_tweets" target="_blank">
                        <li class="twitter-share"><span class="icon-twitter"></span></li>
                    </a>
                    <a href="http://www.facebook.com/sharer.php?u=http://apps.northbynorthwestern.com/commencement/2016/story/<%= entry_slug %>" target="_blank">
                        <li class="facebook-share"><span class="icon-facebook"></span></li>
                    </a>
                </ul>
            </div>
            
        </div>
        <div class="entry-body">
            <%= entry_body %>
        </div>
    </div>
</div>